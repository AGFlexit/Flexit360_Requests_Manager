<?xml version="1.0" encoding="utf-8"?>
<XmlTheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ThemeName="Office2019LightPurple" ThemeVersion="2.0">
  <BuilderRegistrations>
    <XmlStyleBuilderRegistration BuilderType="">
      <BuilderData xsi:type="XmlStyleSheet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <PropertySettingGroups>
          <XmlPropertySettingGroup BasedOn="DefaultFont,MainForeColor">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.RadItem.UseDefaultDisabledPaint" Value="False" />
            </PropertySettings>
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.RootRadElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="MainForeColor,SegoeUI11Font">
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.UI.TileGroupElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="Padding(4;4;4;4),SegoeUI11Font,AccentMouseOverBorderFill,CheckedForeColor,AccentMouseOverBorder2px">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.TextAlignment" Value="TopLeft" />
              <XmlPropertySetting Property="Telerik.WinControls.RadElement.CellPadding" Value="4, 4, 4, 4" />
            </PropertySettings>
            <Selectors>
              <XmlVisualStateSelector VisualState="RadTileElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AlternativeFill">
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.UI.RadPanoramaElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentBorder2px,AccentMouseOverBorderFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadTileElement.MouseOver" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentBorder2px,AccentFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadTileElement.MouseDown" />
            </Selectors>
          </XmlPropertySettingGroup>
        </PropertySettingGroups>
      </BuilderData>
      <StylesheetRelations>
        <RadStylesheetRelation ControlType="Telerik.WinControls.UI.RadPanorama" ElementType="Telerik.WinControls.RootRadElement" />
        <RadStylesheetRelation RegistrationType="ElementTypeDefault" ElementType="Telerik.WinControls.UI.RadPanoramaElement" />
      </StylesheetRelations>
    </XmlStyleBuilderRegistration>
  </BuilderRegistrations>
</XmlTheme>