<?xml version="1.0" encoding="utf-8"?>
<XmlTheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ThemeName="Office2019LightPurple" ThemeVersion="2.0">
  <BuilderRegistrations>
    <XmlStyleBuilderRegistration BuilderType="">
      <BuilderData xsi:type="XmlStyleSheet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <PropertySettingGroups>
          <XmlPropertySettingGroup BasedOn="AlternativeFill">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.VisualElement.SmoothingMode" Value="None" />
            </PropertySettings>
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.UI.SizeGripElement">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="GripFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlTypeSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="MainBorderBottomLeftRight3px,MainBorderBottomLeftRight">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.RadElement.Visibility" Value="Visible" />
            </PropertySettings>
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.UI.SizeGripElement">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="GripBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlTypeSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CollapsedBorder">
            <Selectors>
              <XmlClassSelector ElementClass="GripNSEW">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="GripItemBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlClassSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="GripNSEWImage">
            <Selectors>
              <XmlClassSelector ElementClass="GripNSEW">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="GripNSEWImage" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlClassSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="TransparentFill">
            <Selectors>
              <XmlClassSelector ElementClass="GripNSEW">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="GripItemFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlClassSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="TransparentFill">
            <Selectors>
              <XmlClassSelector ElementClass="GripNS">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="GripItemFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlClassSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="CollapsedBorder">
            <Selectors>
              <XmlClassSelector ElementClass="GripNS">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="GripItemBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlClassSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="GripNSImage">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.RadElement.MaxSize" Value="0, 3" />
            </PropertySettings>
            <Selectors>
              <XmlClassSelector ElementClass="GripNS">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="GripNSImage" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlClassSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="DefaultFont,MainForeColor">
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.RootRadElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.RadElement.MinSize" Value="0, 20" />
              <XmlPropertySetting Property="Telerik.WinControls.RadElement.MaxSize" Value="0, 20" />
            </PropertySettings>
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.UI.SizeGripElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="Padding(1;1;1;1)">
            <Selectors>
              <XmlClassSelector ElementClass="GripNSEW" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="Padding(0;3;0;2)">
            <Selectors>
              <XmlClassSelector ElementClass="GripNS" />
            </Selectors>
          </XmlPropertySettingGroup>
        </PropertySettingGroups>
      </BuilderData>
      <StylesheetRelations>
        <RadStylesheetRelation ControlType="Telerik.WinControls.UI.RadSizablePopupControl" ElementType="Telerik.WinControls.RootRadElement" />
        <RadStylesheetRelation RegistrationType="ElementTypeDefault" ElementType="Telerik.WinControls.UI.SizeGripItem" />
        <RadStylesheetRelation RegistrationType="ElementTypeDefault" ElementType="Telerik.WinControls.UI.SizeGripElement" />
      </StylesheetRelations>
    </XmlStyleBuilderRegistration>
  </BuilderRegistrations>
</XmlTheme>