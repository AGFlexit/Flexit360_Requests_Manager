<?xml version="1.0" encoding="utf-8"?>
<XmlTheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ThemeName="Office2019LightPurple" ThemeVersion="2.0">
  <BuilderRegistrations>
    <XmlStyleBuilderRegistration BuilderType="">
      <BuilderData xsi:type="XmlStyleSheet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <PropertySettingGroups>
          <XmlPropertySettingGroup BasedOn="TransparentFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="SeparatorElement">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="Line2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="MainBorderFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="SeparatorElement">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="Line1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
        </PropertySettingGroups>
      </BuilderData>
      <StylesheetRelations>
        <RadStylesheetRelation ControlType="Telerik.WinControls.UI.RadSeparator" ElementType="Telerik.WinControls.UI.SeparatorElement" />
        <RadStylesheetRelation RegistrationType="ElementTypeDefault" ElementType="Telerik.WinControls.UI.SeparatorElement" />
      </StylesheetRelations>
    </XmlStyleBuilderRegistration>
  </BuilderRegistrations>
</XmlTheme>