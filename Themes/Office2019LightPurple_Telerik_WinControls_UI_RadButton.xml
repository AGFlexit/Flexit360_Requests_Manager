<?xml version="1.0" encoding="utf-8"?>
<XmlTheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ThemeName="Office2019LightPurple" ThemeVersion="2.0">
  <BuilderRegistrations>
    <XmlStyleBuilderRegistration BuilderType="">
      <BuilderData xsi:type="XmlStyleSheet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <PropertySettingGroups>
          <XmlPropertySettingGroup BasedOn="DefaultFont">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.RadItem.UseDefaultDisabledPaint" Value="False" />
            </PropertySettings>
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.RootRadElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentMouseOverFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.MouseOver">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ButtonBackgroundFill">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.VisualElement.SmoothingMode" Value="None" />
            </PropertySettings>
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentPressedFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.MouseDown">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="DefaultFont,Padding(4;4;4;4),MainForeColor">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="DisabledForeColor">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.Disabled" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentPressedFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.Pressed">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ButtonBackgroundFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.IsDefault">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AlternativeFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.Disabled">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonFill" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="MainBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentMouseOverBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.MouseOver">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.MouseDown">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.Pressed">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.IsDefault">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="DisabledBorder">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadButtonElement.Disabled">
                <ChildSelector xsi:type="XmlClassSelector" ElementClass="ButtonBorder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
              </XmlVisualStateSelector>
            </Selectors>
          </XmlPropertySettingGroup>
        </PropertySettingGroups>
      </BuilderData>
      <StylesheetRelations>
        <RadStylesheetRelation ControlType="Telerik.WinControls.UI.RadButton" ElementType="Telerik.WinControls.RootRadElement" />
        <RadStylesheetRelation RegistrationType="ElementTypeDefault" ElementType="Telerik.WinControls.UI.RadButtonElement" />
      </StylesheetRelations>
    </XmlStyleBuilderRegistration>
  </BuilderRegistrations>
</XmlTheme>