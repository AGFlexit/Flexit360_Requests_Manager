<?xml version="1.0" encoding="utf-8"?>
<XmlTheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ThemeName="Office2019LightPurple" ThemeVersion="2.0">
  <BuilderRegistrations>
    <XmlStyleBuilderRegistration BuilderType="">
      <BuilderData xsi:type="XmlStyleSheet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <PropertySettingGroups>
          <XmlPropertySettingGroup BasedOn="DefaultFont,MainForeColor">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.RadItem.UseDefaultDisabledPaint" Value="False" />
              <XmlPropertySetting Property="Telerik.WinControls.VisualElement.BackColor" Value="Transparent" />
            </PropertySettings>
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.UI.RadLabelRootElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="MainForeColor">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.UI.RadLabelElement.TextAlignment" Value="MiddleLeft" />
            </PropertySettings>
            <Selectors>
              <XmlVisualStateSelector VisualState="RadLabelElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="DisabledForeColor">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadLabelElement.Disabled" />
            </Selectors>
          </XmlPropertySettingGroup>
        </PropertySettingGroups>
      </BuilderData>
      <StylesheetRelations>
        <RadStylesheetRelation ControlType="Telerik.WinControls.UI.RadLabel" ElementType="Telerik.WinControls.UI.RadLabelRootElement" />
      </StylesheetRelations>
    </XmlStyleBuilderRegistration>
  </BuilderRegistrations>
</XmlTheme>