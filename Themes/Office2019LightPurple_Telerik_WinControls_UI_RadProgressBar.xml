<?xml version="1.0" encoding="utf-8"?>
<XmlTheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ThemeName="Office2019LightPurple" ThemeVersion="2.0">
  <BuilderRegistrations>
    <XmlStyleBuilderRegistration BuilderType="">
      <BuilderData xsi:type="XmlStyleSheet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <PropertySettingGroups>
          <XmlPropertySettingGroup BasedOn="DefaultFont,MainForeColor">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.RadItem.UseDefaultDisabledPaint" Value="False" />
            </PropertySettings>
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.RootRadElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="MainForeColor,MainBorderFill">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.DrawBorder" Value="False" />
            </PropertySettings>
            <Selectors>
              <XmlVisualStateSelector VisualState="RadProgressBarElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="UpperProgressIndicatorElement.IsVertical" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentMouseOverFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="ProgressIndicatorElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentMouseOverFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="ProgressIndicatorElement.IsVertical" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AlternativeFill,DisabledForeColor">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadProgressBarElement.Disabled" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="AccentFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="UpperProgressIndicatorElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="SecondaryFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="UpperProgressIndicatorElement.Disabled" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ButtonBackgroundFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="ProgressIndicatorElement.Disabled" />
            </Selectors>
          </XmlPropertySettingGroup>
        </PropertySettingGroups>
      </BuilderData>
      <StylesheetRelations>
        <RadStylesheetRelation ControlType="Telerik.WinControls.UI.RadProgressBar" ElementType="Telerik.WinControls.RootRadElement" />
        <RadStylesheetRelation RegistrationType="ElementTypeDefault" ElementType="Telerik.WinControls.UI.RadProgressBarElement" />
      </StylesheetRelations>
    </XmlStyleBuilderRegistration>
  </BuilderRegistrations>
</XmlTheme>